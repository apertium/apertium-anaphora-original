<?xml version="1.0" encoding="UTF-8"?> <!-- -*- nxml -*- -->
<ref>
	
	<section-parameters>
		
		<def-parameter n="anaphor">
			<parameter-item tags="det.pos.*"/>
			<parameter-item tags="prn.*"/>
		</def-parameter>

		<def-parameter n="antecedent">
			<parameter-item tags="n.*"/>
		</def-parameter>

	</section-parameters>

	<section-def-cats>
		
		<def-cat n="det">
			<cat-item tags="det.*"/>
			<cat-item tags="det.pos.*"/>
		</def-cat>

		<def-cat n="adj">
			<cat-item tags="adj.*"/>
		</def-cat>

		<def-cat n="nom">
			<cat-item tags="n.*"/>
		
		</def-cat>
	</section-def-cats>

	<section-markables>
		
		<markable n="NP">
			<pattern>
				<pattern-item n="det"/>
				<pattern-item n="adj"/>
				<pattern-item n="nom" head="t"/>
			</pattern>
			<pattern>
				<pattern-item n="det"/>
				<pattern-item n="nom" head="t"/>
			</pattern>
		</markable>
		
	</section-markables>
	
</ref>